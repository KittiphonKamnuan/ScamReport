# ===========================================
# ScamReport Frontend Environment Variables
# Copy this file to .env and fill in your values
# ===========================================

# AWS Region
VITE_REGION=us-east-1

# AWS Cognito (Optional - currently not in use)
# VITE_USER_POOL_ID=your-user-pool-id
# VITE_CLIENT_ID=your-client-id

# API Configuration
# Lambda Function URL
VITE_API_BASE_URL=https://your-lambda-url.lambda-url.us-east-1.on.aws

# ===========================================
# Development Only - Test Credentials
# Fill in for development mode only
# ===========================================

# Admin Test Account
VITE_TEST_ADMIN_EMAIL=admin@example.com
VITE_TEST_ADMIN_PASSWORD=

# Journalist Test Account
VITE_TEST_JOURNALIST_EMAIL=journalist@example.com
VITE_TEST_JOURNALIST_PASSWORD=
